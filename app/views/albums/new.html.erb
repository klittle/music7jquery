<% form_for @album, :html => {:multipart => true} do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  
  <p>
    <%#= f.label :photo %>
    <%#= f.file_field :photo %>
  </p>

  <%= f.submit "Create" %>
<% end %>


<p><%= link_to 'Artists', artists_path %></p>
<p><%= link_to 'Albums', albums_path %></p>

